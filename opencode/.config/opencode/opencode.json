{
  "$schema": "https://opencode.ai/config.json",
  "theme": "system",
  "disabled_providers": ["openrouter", "openai"],
  "model": "chatanywhere/gpt-5.1-codex",
  "small_model": "github-copilot/claude-haiku-4-5",
  "provider": {
    "chatanywhere": {
      "npm": "@ai-sdk/openai",
      "name": "ChatAnywhere",
      "options": {
        "baseURL": "{env:CHATANYWHERE_BASEURL}",
        "apiKey": "{env:CHATANYWHERE_API_KEY}"
      },
      "models": {
        "gpt-5.1-chat-latest": {
          "name": "gpt-5.1-chat-latest"
        },
        "gpt-5.1-codex": {
          "name": "gpt-5.1-codex"
        },
        "gpt-5-mini": {
          "name": "gpt-5-mini"
        }
      }
    }
  }
}
